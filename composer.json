{
    "name": "202-ecommerce/stripe_official",
    "description": "Official Stripe PrestaShop module",
    "homepage": "https://github.com/202-ecommerce/stripe_official",
    "authors": [
        {
            "name": "202-ecommerce",
            "email": "contact@202-ecommerce.com"
        }
    ],
    "type": "prestashop-module",
    "config": {
        "prepend-autoloader": false
    },
    "require": {
        "stripe/stripe-php": "^7.33"
    },
    "autoload": {
        "psr-4": {
          "Stripe_officialClasslib\\": "vendor/totpsclasslib/src/"
        },
        "classmap": [
            "classes/"
        ]
    },
    "scripts": {
        "pre-autoload-dump": "if [ ${TOTPSCLASSLIB_DEV_PATH} ]; then php ${TOTPSCLASSLIB_DEV_PATH}/classlib/refresh.php .; fi"
    }
}
